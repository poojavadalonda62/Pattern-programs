class  Num8
{
	public static void main(String[] args) 
	{
		int n=1;
		char ch='a';
		for (int i=1;i<=5 ;i++ )
		{
			for (int j=1;j<=5 ;j++ )
			{
				if (i==j)
				{
					System.out.print(n+++" ");
				}
				else if (i+j==6)
				{
					System.out.print(ch+++" ");
				}
				else
				{
					System.out.print("*"+" ");
				}

			}
		
		System.out.println();
		}
	}
}
