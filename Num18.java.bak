class  Num18
{
	public static void main(String[] args) 
	{
		int n=1;

		for (int i=1;i<=5 ;i++ )
		{
			for (int j=1;j<=5 ;j++ )
			{

			  if ( n%2!=0)||i==1||i==5||j==1||j==5||i==2&&j==4||i==3&&j==3||i==4&&j==2)
				{
					System.out.print(n+" ");
				}
				else
				{
					System.out.print(" "+" ");
				}
			}
		System.out.println();
		}
	}
	
}
