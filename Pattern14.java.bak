class  Pattern14
{
	public static void main(String[] args) 
	{
		int n=1;
		int ch='a';
		for (int i=1;i<=4 ;i++ )
		{
			for (int j=1;j<=4 ;j++ )
			{
				if (i==2||i==4)
				{
					System.out.print(ch+++" ");
				}
				else
				{
					System.out.print(n+++" ");
				}
			}
		
		System.out.println();
		}
	}
}
