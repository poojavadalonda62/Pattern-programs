class  Num16
{
	public static void main(String[] args) 
	{
		int n=1;
		for (int i=1;i<=5 ;i++ )
		{
			for (int j=1;j<=5 ;j++ )
			{
				if (i==1||i==5||i==j)
				{
					System.out.print(n+++" ");
				}
				else
				{
					System.out.print(" "+" ");
				}
				if (n>9)
				{
					n=1;
				}
			}
		
		System.out.println();
		}
	}
}
